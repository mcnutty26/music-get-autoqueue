{
  "manifest_version": 2,

  "name": "music-get-autoqueue",
  "description": "Clicking the button queues whatever's at the current tab's URL to the music server.",
  "version": "1.2",
  "short_name": "music-get",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://music.lan/"
  ],
  "icons": { "16": "icon16.png",
	         "48": "icon32.png",
			 "128": "icon96.png" 
  }
}

